<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:l="sap.ui.layout"
	controllerName="asaod.ASAOD.controller.Admin" height="100%">
	<Dialog title="Assigned DLs" contentHeight="500px" contentWidth="150px" id="showdls">
		<content>
			<List id="ShortProductList" headerText="" items="{dllist>/results}">
				<items>
					<StandardListItem title="{dllist>Dl}"/>
				</items>
			</List>
		</content>
		<endButton>
			<Button text="Close" press="closeDlListDialog"></Button>
		</endButton>
	</Dialog>
</core:FragmentDefinition>