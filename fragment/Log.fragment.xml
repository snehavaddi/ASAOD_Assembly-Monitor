<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" class="sapUiSizeCompact" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
	<Dialog title="Details" icon="sap-icon://form">
		<content>
			<TextArea value="{logDetails>/Line}" width="100%" editable="false" height="auto" growing="true"/>
		</content>
		<endButton>
			<Button press="closeLogDialog" text="Close" tooltip="{/ItemShortText}"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>